{
    "name": "deloughry/ai-ipsum",
    "description": "AI-powered content generator built with Laravel Zero for creating intelligent dummy content using various AI providers.",
    "keywords": ["ai", "content-generation", "laravel-zero", "console", "cli", "dummy-content", "ai-providers", "prism"],
    "homepage": "https://deloughry.co.uk",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Mattew Deloughry",
            "email": "matt@deloughry.co.uk"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel-zero/framework": "^12.0",
        "laravel-zero/phar-updater": "^1.4",
        "prism-php/prism": "^0.86.0"
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["ai-ipsum"]
}
